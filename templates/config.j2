{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": {{ v2ray_port }},
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "{{ v2ray_uuid }}",
            "alterId": {{ v2ray_alterId }},
            "level": 0
          }
        ]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
